   RowBox[{"cartesiantopolar", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], " ", ":=", " ", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"y", "<", "0"}], ",", 
     RowBox[{"{", 
