   3.666156373302506*^9}},ExpressionUUID->"7430ff3b-084e-42c2-881a-\
fa42087787ca"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"anagramQ", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"StringMatchQ", "[", 
    RowBox[{
     RowBox[{"StringJoin", "[", 
      RowBox[{"ToLowerCase", "[", 
       RowBox[{"Sort", "[", 
        RowBox[{"Characters", "[", "x", "]"}], "]"}], "]"}], "]"}], ",", 
     RowBox[{"StringJoin", "[", 
      RowBox[{"ToLowerCase", "[", 
       RowBox[{"Sort", "[", 
        RowBox[{"Characters", "[", "y", "]"}], "]"}], "]"}], "]"}]}], "]"}]}],
   ";"}]], "Input",
 CellChangeTimes->{{3.7790453418019843`*^9, 3.7790453851909723`*^9}, {
