Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"palindromeQ", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{"StringMatchQ", "[", 
    RowBox[{
     RowBox[{"ToLowerCase", "[", 
      RowBox[{"StringReplace", "[", 
       RowBox[{"x", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\< \>\"", "\[Rule]", "\"\<\>\""}], ",", 
          RowBox[{"\"\<.\>\"", "\[Rule]", " ", "\"\<\>\""}], ",", 
          RowBox[{"\"\<,\>\"", "\[Rule]", " ", "\"\<\>\""}], ",", 
          RowBox[{"\"\<!\>\"", "\[Rule]", " ", "\"\<\>\""}]}], "}"}]}], "]"}],
       "]"}], ",", 
     RowBox[{"StringReverse", "[", 
      RowBox[{"ToLowerCase", "[", 
       RowBox[{"StringReplace", "[", 
