 RowBox[{
  RowBox[{
   RowBox[{"anagramQ", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", " ", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"StringQ", "[", "x", "]"}], " ", "&&", "  ", 
      RowBox[{"StringQ", "[", "y", "]"}]}], ",", 
     RowBox[{"StringMatchQ", "[", 
      RowBox[{
       RowBox[{"StringJoin", "[", 
        RowBox[{"ToLowerCase", "[", 
         RowBox[{"Sort", "[", 
          RowBox[{"Characters", "[", "x", "]"}], "]"}], "]"}], "]"}], ",", 
       RowBox[{"StringJoin", "[", 
        RowBox[{"ToLowerCase", "[", 
         RowBox[{"Sort", "[", 
          RowBox[{"Characters", "[", "y", "]"}], "]"}], "]"}], "]"}]}], "]"}],
