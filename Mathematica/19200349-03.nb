          RowBox[{"\"\< \>\"", "\[Rule]", "\"\<\>\""}], ",", 
          RowBox[{"PunctuationCharacter", "\[Rule]", " ", "\"\<\>\""}]}], 
         "}"}]}], "]"}], "]"}], ",", 
     RowBox[{"StringReverse", "[", 
      RowBox[{"ToLowerCase", "[", 
       RowBox[{"StringReplace", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\"\< \>\"", "\[Rule]", "\"\<\>\""}], ",", 
           RowBox[{"PunctuationCharacter", "\[Rule]", " ", "\"\<\>\""}]}], 
          "}"}]}], "]"}], "]"}], "]"}]}], "]"}]}], ";"}]], "Input",
