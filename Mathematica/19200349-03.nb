Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"palindromeQ", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"StringQ", "[", "x", "]"}], ",", 
     RowBox[{"StringMatchQ", "[", 
      RowBox[{
       RowBox[{"ToLowerCase", "[", 
        RowBox[{"StringReplace", "[", 
         RowBox[{"x", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"\"\< \>\"", "\[Rule]", "\"\<\>\""}], ",", 
            RowBox[{"PunctuationCharacter", "\[Rule]", " ", "\"\<\>\""}]}], 
           "}"}]}], "]"}], "]"}], ",", 
       RowBox[{"StringReverse", "[", 
        RowBox[{"ToLowerCase", "[", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"x", ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\"\< \>\"", "\[Rule]", "\"\<\>\""}], ",", 
             RowBox[{"PunctuationCharacter", "\[Rule]", " ", "\"\<\>\""}]}], 
            "}"}]}], "]"}], "]"}], "]"}]}], "]"}], ",", 
